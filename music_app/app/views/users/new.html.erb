<h2>New User Form</h2>

<form action="<%= users_url %>" method="post">
  
  <label>
    Email:
    <input type="text" name="user[email]" value="">
  </label>
  <br><br>
  
  <label>
    Password:
    <input type="text" name="user[password]" value="">
  </label>
  <br><br>
  
  <input type="submit" value="Create Account">
  
  <p><%= flash.now[:user] %></p>
  
</form>